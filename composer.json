{
    "name": "hauntpet/theme",
    "description": "The main theme package.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Lumite Studios",
            "email": "hauntpet@lumitestudios.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.17"
    },
    "autoload": {
        "psr-4": {
            "HauntPet\\Theme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HauntPet\\Theme\\ThemeServiceProvider"
            ],
            "aliases": {
                "ThemeRepository": "HauntPet\\Theme\\Facades\\Repository"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
